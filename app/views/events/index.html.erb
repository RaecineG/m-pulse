<!-- Background Map -->
<div class="background-map"
style="position: relative; width: 100vw; height: 100vh;"
data-controller="map"
data-map-markers-value="<%= @markers.to_json %>"
data-map-followee-markers-value="<%= @followee_markers.to_json %>"
data-map-api-key-value="<%= ENV['MAPBOX_API_KEY'] %>">
  <div class="map-details" data-map-target="details"></div>
</div>
<!-- current location-->
<div data-controller="current-location"></div>
